#include<iostream>
using namespace std;
int main()
{
    int t,n,c=0;
    cin>>t;
    while(t--)
    {   cin>>n;
    c=0;
        for(int i=1;i<=n;i++)
        {
            for(int j=i+1;j<=n;j++)
            {
                if((i^j)<=n)
                {
                    c++;
                }
            }
        }
        cout<<c<<endl;
    }
}
