#include<iostream>
#include<string.h>
using namespace std;
int main(){
    long long int t,l,c=0;
    cin>>t;
    string a;
    for(int i=0;i<t;i++)
    {
        cin>>a;
        l=a.length();
        
        for(long long int j=l;j>0;j--)
        {   
            c=c+j;;
        }
        cout<<c<<"\n";
        c=0;
    }
}
